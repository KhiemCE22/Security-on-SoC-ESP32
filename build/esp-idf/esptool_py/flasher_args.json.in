{
    "write_flash_args" : [ "--flash_mode", "dio",
                           "--flash_size", "4MB",
                           "--flash_freq", "40m" ],
    "flash_settings" : {
        "flash_mode": "dio",
        "flash_size": "4MB",
        "flash_freq": "40m"
    },
    "flash_files" : {
        "0x20000" : "$<TARGET_PROPERTY:__idf_build_target,EXECUTABLE_NAME>.bin",
        "0x10000" : "partition_table/partition-table.bin"
    },
    "app" : { "offset" : "0x20000", "file" : "$<TARGET_PROPERTY:__idf_build_target,EXECUTABLE_NAME>.bin", "encrypted" : "true" },
    "partition-table" : { "offset" : "0x10000", "file" : "partition_table/partition-table.bin", "encrypted" : "true" },
    "extra_esptool_args" : {
        "after"  : "no_reset",
        "before" : "default_reset",
        "stub"   : true,
        "chip"   : "esp32"
    }
}
